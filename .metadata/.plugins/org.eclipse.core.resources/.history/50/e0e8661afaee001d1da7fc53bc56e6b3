public class App 
{
    public static void main( String[] args )
    {
    	Configuration configuration = new Configuration();
    	configuration.configure("hibernate.cfg.xml");
    	SessionFactory buildSessionFactory = configuration.buildSessionFactory();
    	
    }
}
