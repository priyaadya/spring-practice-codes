import org.springframework.web.bind.annotation.ResponseBody;

@Controller
public class TestController {
	
	@RequestMapping("/test")
	@ResponseBody
	public String test() {
		int a=70;
		int b=20;
		int c=90;
		return "this is just  just testing:"+ (a + b);
		
	}

}
